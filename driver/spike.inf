[Strings]
DriverPackageDisplayName="LEGO Technic Large Hub in FS Mode"
ManufacturerName="Driver for Spike Prime"
ServiceName="USB RS-232 Emulation Driver"
description="Spike USB COM Port"

[DefaultInstall]
CopyINF=spike.inf

[Version]
Class=Ports
ClassGuid={4D36E978-E325-11CE-BFC1-08002BE10318}
Signature="$Windows NT$"
Provider=%ManufacturerName%
DriverPackageDisplayName=%DriverPackageDisplayName%
'CatalogFile=NotSigned.cat
DriverVer=09/09/2016,1.1.1.0

[Manufacturer]
%ManufacturerName%=DeviceList,NTamd64,NTia64

[DestinationDirs]
FakeModemCopyFileSection=12
DefaultDestDir=12

[DeviceList]
%description%=DriverInstall,USB\VID_0694&PID_0009&REV_0200,USB\VID_0694&PID_0009&REV_0200

[DeviceList.NTamd64]
%description%=DriverInstall,USB\VID_0694&PID_0009&REV_0200,USB\VID_0694&PID_0009&REV_0200

[DeviceList.NTia64]
%description%=DriverInstall,USB\VID_0694&PID_0009&REV_0200,USB\VID_0694&PID_0009&REV_0200

[DriverInstall]
include=mdmcpq.inf,usb.inf
CopyFiles=FakeModemCopyFileSection
AddReg=DriverAddReg

[DriverAddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,usbser.sys
HKR,,EnumPropPages32,,"MsPorts.dll,SerialPortPropPageProvider"

[DriverInstall.Services]
include=mdmcpq.inf
AddService=usbser,0x00000002,DriverService

[DriverService]
DisplayName=%ServiceName%
ServiceType=1
StartType=3
ErrorControl=1
ServiceBinary=%12%\usbser.sys
LoadOrderGroup=Base
